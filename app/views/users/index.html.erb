<h2>Listing current users</h2>
<table>
	<tr><th>Email</th><th>Delete</th></tr>
	<%@users.each do |user|%>
	<tr><td><%=user.name%></td>
		<td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		</tr>
	<%end%>
	<%@proxies.each do |proxy| %>
	<tr><td><%= proxy.email %></td>
		<td><%= link_to 'Destroy', proxy, method: :delete, data: {confirm: 'Are you sure?'}%></td>
	</tr>
	<%end%>
	</table>